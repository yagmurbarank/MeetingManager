@{
}
@model List< MeetingManager.Models.Entities.Meeting>

<h1>Meetings</h1>
<table class="table">
<thead>
   <tr>
       <th> Title</th>
       <th>Time</th>
       <th>Location</th>
       <th>Description</th>
   </tr>
</thead>
<tbody>
        @foreach(var meeting in Model)
        {
            <tr>
                <th>@meeting.Id</th>
                <th>@meeting.Title</th>
                <th>@meeting.Time</th>
                <th>@meeting.Location</th>
                <th>@meeting.Description</th>
                <td>
                   <a class="btn-btn-primary" asp-controller="Meetings" asp-action="Edit" asp-all-route-id="@meeting.Id">Edit</a>
                </td>
            </tr>
            
        }

</tbody>
</table>